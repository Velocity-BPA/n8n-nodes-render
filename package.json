{
  "name": "n8n-nodes-render",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Render Network providing 17 resources and 150+ operations for GPU rendering, AI compute, wallet management, staking, and network statistics on Solana blockchain.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "render",
    "render-network",
    "gpu",
    "3d-rendering",
    "ai-compute",
    "solana",
    "blockchain",
    "decentralized",
    "octane",
    "gpu-compute",
    "staking",
    "defi"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-render",
  "author": {
    "name": "Velocity BPA, LLC",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-render.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RenderNetwork.credentials.js",
      "dist/credentials/RenderApi.credentials.js",
      "dist/credentials/NodeOperator.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Render/Render.node.js",
      "dist/nodes/Render/RenderTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@solana/spl-token": "^0.3.11",
    "@solana/web3.js": "^1.89.1",
    "axios": "^1.6.2",
    "bs58": "^5.0.0",
    "form-data": "^4.0.0",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
